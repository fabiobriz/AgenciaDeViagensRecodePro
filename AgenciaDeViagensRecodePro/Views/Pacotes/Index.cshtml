@model IEnumerable<AgenciaDeViagensRecodePro.Models.Pacote>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Tipo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkClienteIdClienteNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkHospedagemIdHospedagemNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FkPassagemIdPassagemNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tipo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkClienteIdClienteNavigation.IdCliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkHospedagemIdHospedagemNavigation.IdHospedagem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FkPassagemIdPassagemNavigation.IdPassagem)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPacote">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdPacote">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdPacote">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
